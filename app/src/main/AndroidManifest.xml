<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dipu.testmodule">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">

        </activity>

        <activity android:name=".UserEntryActivity"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".QuestionairActivity"
            android:label="Questionnaire"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ReportActivity"
            android:label="Reports"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ModuleList" />
        <activity android:name=".EmployeeDetailsActivity" />
        <activity android:name=".ManagerActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".UserProfileActivity"
            android:label="Your Profile"></activity>
        <activity
            android:name=".Aftertestcomplete"
            android:label="Test Report"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>